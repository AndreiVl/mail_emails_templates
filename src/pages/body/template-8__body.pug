      table(width="100%" align="center" border="0" cellspacing="0" cellpadding="0")
        tr
          +td-margin(40)
        tr
          td(align="center" style="padding-bottom: 5px")
            p.text(style="font-size: 12px; line-height: 21px; font-weight: 400; color: #818181; text-align: center;")
              span Some Firm ltd. 35 Avenue. City 10115, USA
                br
                a(href="#" target="_blank" style=" color: #818181; text-decoration: underline; ") Unsubscribe
                |  from our emails
        tr
          +td-margin(58)
    +td-width('3.65%')
