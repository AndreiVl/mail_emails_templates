<!-- HEADER-1 STARTS -->
table(align="center" border="0" cellpadding="0" cellspacing="0" width="100%")
	tr
		td(align="center")
			+IE({width: table_width ,aling: 'center', valing: 'top'})
			div(style="display: inline-block; width: 100%; max-width: " + table_width + "px; vertical-align: top;")
				<!-- ID:BG HEADER-1 OPTIONAL -->
				table(align="center" bgcolor=bg_table border="0" cellpadding="0" cellspacing="0" width="100%" style="max-width: " + table_width + "px;")
					tr
						td(align="center" style="padding-left: " + body_pading_l + "px; padding-right: " + body_pading_r + "px;")
							+IE({width: body_width ,aling: 'center', valing: 'top'})
							div(style="display: inline-block; width: 100%; max-width: " + body_width + "px; vertical-align: top;")
								table(align="left" border="0" class="display-width-inner" cellpadding="0" cellspacing="0" width="100%" style="max-width: " + body_width + "px;")
									tr
										+td-margin({margin: 'mr-red'})
									tr
										td(align="left" style="color: #333333;" width="92")
											a.link(href="#")
												img(src=image_path+"logo__92-01.png" alt="logo" width="92" height="30" style="display: block; padding: 0; margin: 0; border: 0;")
							|!{IE_TABLE_END}
			|!{IE_TABLE_END}
<!-- HEADER-1 ENDS -->