mixin profile(data)
	table(align="center" border="0" cellpadding="0" cellspacing="0" width="100%" style="border-collapse: collapse; mso-table-lspace: 0pt; mso-table-rspace: 0pt; width: auto !important;")
		tbody
			tr
				td(align="center" style="color:#666666; line-height:0px;" width="268")
					img(src=image_path+data.img, border="0" alt=data.alt width="268" height="278" style="display: block; width: 100%; max-width: 268px; height: auto; margin: 0; border: 0; padding: 0; outline: none;")
			tr
				+td-margin({margin: 'mr-green'})
			tr
				td.h2(align="center")!=data.title
			tr
				td.lead(align="center")!=data.lead
			tr
				+td-margin({margin: 'mr-yelow'})
			tr
				td(align="center" valign="top")
					a(href="#" target="_blank")
						img(src=image_path+"btn__90_32-01.png" border="0" alt="btn"  width="90" height="32" style="display: block; width: 100%; max-width: 90px; height: auto; margin: 0; border: 0; padding: 0; outline: none;")
			tr
				+td-margin({margin: 'mr-green'})